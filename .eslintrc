{
    "extends": [
        "wesbos"
    ],
    "rules": {
        "no-console": 2,
        "prettier/prettier": [
            "error",
            {
                "trailingComma": "es5",
                "printWidth": 80,
                "tabWidth": 4,
                "singleQuote": true,
                "bracketSpacing": false,
                "jsxBracketSameLine": false,
                "proseWrap": "always"
            }
        ]
    }
}